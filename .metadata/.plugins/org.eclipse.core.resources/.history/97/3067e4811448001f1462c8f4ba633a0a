package Methods;

import java.util.Scanner;

public class Marraige {
		public static int Eligible( int expenditure, int revenue) {
			int profit = revenue - expenditure;
			return profit;
			
		}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("what is the revenue?");
		int r = sc.nextInt();
		System.out.println("and what is the expenditure?");
		int e = sc.nextInt();
		Marraige.Eligible(r, e);
		sc.close();
	}

}
